<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meetofinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AlternativeTravel"></activity>
        <activity android:name=".MatchInProgress" />
        <activity android:name=".ConfirmTrip" />
        <activity android:name=".Chat" />
        <activity android:name=".MatchedTripSummary" />
        <activity android:name=".UpcomingTrips" />
        <activity android:name=".TravelSaveTrip" />
        <activity android:name=".TravelReturnTrip" />
        <activity android:name=".TravelRoundTripQuestion" />
        <activity android:name=".TravelOneWay" />
        <activity android:name=".StartPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>